{
"manifest_version": 2,

  "name": "Chrome Tab Disguise",
  "description": "An extension that Diguises the favicon and URL of the Tab",
  "version": "1.0",

  "permissions": [
	"tabs",
	"activeTab",
        "http://*/",
        "https://*/"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "menu.html"
  }
}
